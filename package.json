{
  "name": "configcat-common",
  "version": "7.0.1",
  "description": "ConfigCat is a configuration as a service that lets you manage your features and configurations without actually deploying new code.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "module": "lib/esm/index.js",
  "scripts": {
    "coverage": "nyc npm run test",
    "build": "tsc -p tsconfig.build.cjs.json && tsc -p tsconfig.build.esm.json",
    "prepare": "npm run build",
    "test": "cross-env TS_NODE_PROJECT=./tsconfig.mocha.json node --expose-gc node_modules/mocha/bin/_mocha --require ts-node/register 'test/**/*.ts' --exit --timeout 30000",
    "test-debug": "cross-env TS_NODE_PROJECT=./tsconfig.mocha.json node --expose-gc node_modules/mocha/bin/_mocha --fgrep \"TITLE_OF_TEST\" --require ts-node/register 'test/**/*.ts' --exit --timeout 30000"
  },
  "keywords": [
    "configcat",
    "config",
    "configuration",
    "remote configuration",
    "configcat client",
    "feature flags",
    "feature toggle",
    "feature switch",
    "canary release",
    "soft launch",
    "release strategy"
  ],
  "author": "ConfigCat",
  "license": "MIT",
  "homepage": "https://configcat.com",
  "dependencies": {
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@types/chai": "4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "chai": "^4.3.7",
    "cross-env": "^7.0.3",
    "mocha": "^10.2.0",
    "moq.ts": "^7.4.1",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/configcat/common-js"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "json",
      "lcov"
    ],
    "all": true,
    "include": [
      "src"
    ],
    "exclude": [
      "src/Semver.ts",
      "src/Sha1.ts"
    ]
  },
  "sideEffects": false
}
