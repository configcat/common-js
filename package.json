{
  "name": "configcat-common",
  "version": "5.3.0",
  "description": "ConfigCat is a configuration as a service that lets you manage your features and configurations without actually deploying new code.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "module": "lib/esm/index.js",
  "scripts": {
    "coverage": "nyc npm run test",
    "build": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json && tsc -p tsconfig.legacy.json",
    "prepare": "npm run build",
    "test": "mocha --require ts-node/register test/**/*.ts --exit --timeout 30000",
    "test-debug": "mocha --fgrep \"TITLE_OF_TEST\" --require ts-node/register test/**/*.ts --exit --timeout 30000"
  },
  "keywords": [
    "configcat",
    "config",
    "configuration",
    "remote configuration",
    "configcat client",
    "feature flags",
    "feature toggle",
    "feature switch",
    "canary release",
    "soft launch",
    "release strategy"
  ],
  "author": "ConfigCat",
  "license": "MIT",
  "homepage": "https://configcat.com",
  "devDependencies": {
    "@types/chai": "4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.31",
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "moq.ts": "^7.4.1",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/configcat/common-js"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "json",
      "lcov"
    ],
    "all": true,
    "include": [
      "src"
    ],
    "exclude": [
      "src/Semver.ts",
      "src/Sha1.ts"
    ]
  },
  "sideEffects": false
}
